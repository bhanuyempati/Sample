<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <validationRules>
        <fullName>Account_Number_8_Characters_Test</fullName>
        <active>false</active>
        <description>Validates that the Account Number is numeric if not blank. alsjoisbda</description>
        <errorConditionFormula>LEN( AccountNumber) != 8</errorConditionFormula>
        <errorMessage>Account Number is not numeric.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>NumberofLocationsRule_1</fullName>
        <active>true</active>
        <errorConditionFormula>ISBLANK(NumberofLocations__c)</errorConditionFormula>
        <errorMessage>validation failed</errorMessage>
    </validationRules>
</CustomObject>
